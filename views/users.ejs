<!doctype html>
<html>
<head>
    <% include partials/head %>
</head>

<body>
    <div class="container">

        <div class="col-sm-6 col-sm-offset-3">

            <h1><span class="fa fa-sign-in"></span> User List</h1>

            <!-- USER TABLE -->

            <table class="table">
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Email</th>
                </tr>
                <% users.forEach(function(user) { %>
                <tr>
                    <td><%= user._id %></td>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                </tr>
                <% }) %>
            </table>
        </div>
    </div>
</body>

</html>