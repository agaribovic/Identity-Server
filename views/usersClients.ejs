<!doctype html>
<html>

<head>
    <% include partials/head %>
    <% include partials/customCss %>
</head>

<body>

    <div class="container admin-panel-form">
        <div class="custom-container">
            <h2 align="center">Client List</h2>
            <div class="row">
                <div class="col-md-6">
                    <a href="/adminUserView"><i class="fas fa-chevron-left"></i> Back</a>
                </div>
            </div>

            <table class="table custom-table mt-20">
                <tr>
                    <th>Date Created</th>
                    <th>ClientName</th>
                    <th>Redirect</th>
                </tr>
                <% props.forEach(function(item) { %>
                <tr>
                    <td><%= item.client.stringCreated %></td>
                    <td><%= item.client.name %></td>
                    <td class="client-redirect"><%= item.client.redirect %></td>
                </tr>
                <% }) %>
            </table>

            <div class="footer mt-40">
                <div class="col-md-6 offset-md-3 text-center">
                    <p>Â© 2019 Copyright By Apollo Team</p>
                </div>
            </div>
        </div>
    </div>
</body>

</html>